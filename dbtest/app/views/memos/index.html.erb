<p id="notice"><%= notice %></p>

<h1>Listing Memos</h1>

<table>
  <thead>
    <tr>
      <th>Student name</th>
      <th>Student id</th>
      <th>Student academy</th>
      <th>Student major</th>
      <th>Student degree</th>
      <th>Student credit</th>
      <th>Student course</th>
      <th>Student password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @memos.each do |memo| %>
      <tr>
        <td><%= memo.student_name %></td>
        <td><%= memo.student_id %></td>
        <td><%= memo.student_academy %></td>
        <td><%= memo.student_major %></td>
        <td><%= memo.student_degree %></td>
        <td><%= memo.student_credit %></td>
        <td><%= memo.student_course %></td>
        <td><%= memo.student_password %></td>
        <td><%= link_to 'Show', memo %></td>
        <td><%= link_to 'Edit', edit_memo_path(memo) %></td>
        <td><%= link_to 'Destroy', memo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Memo', new_memo_path %>
